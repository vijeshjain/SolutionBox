       <table><tr>
            <div class="texbox"><p style = "font-weight:bold;">&nbsp;&nbsp;
                Your Question: <%= q%> ? </p>
                </div></tr> 
          
         <% if(text.length)
          {
           
            for(var i=0; i < title.length ; i++) 
                {   
                    
                     var title1 = title[i];
                     title[i] = title1.replace(/[&#,$~%.'":;*<>39]/g,'');
                      %>
                <br>
                <br>
                <div class="textox"><p style = "font-weight:bold;">&nbsp;&nbsp;
                <%= title[i] %></p>
                </div>
                <%
                    var text1 = "";
                      text1 = text[i];
                    <!-- text[i] = text1.replace(/[&#,$~%.'":;*<>39]/g,''); -->

                        var str1 = new Array();
                    str1 = text1.split("\n");
                %>
              
                 <div style="font-weight: bold; border: 2px solid #2C3E50; margin-top: 10px;box-shadow: 5px 5px 6px 0px #2C3E50;border-radius: 5px;">&nbsp;&nbsp;
                    <div style="font-size: 18px; overflow: hidden;">
                        <% for(var j=0; j<str1.length; j++) { 
                                str1[j] = str1[j].replace('&lt;','<');
                                str1[j] = str1[j].replace('&gt;','>');
                                str1[j] = str1[j].replace(/&#39;/g,"'");
                                str1[j] = str1[j].replace(/&quot;/g,"%"); %>
                                <span><%= str1[j] %></span><br/>
                                <% console.log(str1[j]); } %>
                    </div></p>
                <button type="button" class="btn btn-primary" onclick="location.href='/allanswers/<%= results.items[i].question_id %>'" style="height: 40px;width: 120px;
                    border-radius: 2pc;text-align: center;padding-bottom: 30px;margin-bottom: 5px;">
                    All Answers</button>
                 <button type="button" class="btn btn-primary" onclick="location.href='/bestanswer/<%= results.items[i].question_id %>'" style="height: 40px;width: 120px;
                    border-radius: 2pc;text-align: center;padding-bottom: 30px;margin-bottom: 5px;">
                    Best Answers</button>

                </div>
                <% 
                } 
        }%>
